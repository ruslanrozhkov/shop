<% @products.each do |product| %>
    <strong>Категория:</strong>
    <%= product.category.name %><br>
    <strong>Заголовок:</strong>
    <%= product.title %><br>
    <%= link_to 'Редактировать', edit_admin_product_path(product) %> |
    <%= link_to  'Изображения', admin_product_pictures_path(product) %><br>
<% end %>